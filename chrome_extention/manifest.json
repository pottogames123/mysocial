{
    "manifest_version" : 2,
    "name" : "All In Social Extension",
    "version" : "1.0",
    "description" : "Chose Your Elias Port, And Start Now With Our Social Media!",
    "icons" : {
        "128" : "icon.png",
        "48" : "icon.png",
        "16" : "icon.png"
    },
    "browser_action" : {
        "default_icon" : "icon.png",
        "default_popup" : "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}